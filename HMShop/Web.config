<?xml version="1.0"?>
<configuration>
  <appSettings/>
  <connectionStrings>
    <clear/>
    <!--On Company-->
    <!--<add name="HMShopDB" providerName="System.Data.SqlClient" connectionString="Server=DELL-THAOND;Database=HMShopDB;uid=sa;pwd=123a@;"/>-->
    <!--On Home-->
    <add name="HMShopDB" providerName="System.Data.SqlClient" connectionString="Server=DELL-LAP;Database=HMShopDB;uid=sa;pwd=123a@;"/>
  </connectionStrings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6" />
      </system.Web>
  -->
  <system.web>
    <!--Đếm số người online -->
    <sessionState mode="InProc" cookieless="false" timeout="20"/>
    <!--<pages maintainScrollPositionOnPostBack="true" />-->
    <!-- Turn On/Off Custom Errors -->
    <customErrors mode="Off" defaultRedirect="~/LogPages/DefaultRedirectErrorPage.aspx">
      <error statusCode="404" redirect="~/LogPages/Http404ErrorPage.aspx"/>
      <error statusCode="500" redirect="~/LogPages/ServerError.aspx"/>
    </customErrors>
    <authentication mode="Forms">
      <forms name=".HMShopFormAuth" path="/" protection="All" timeout="30" slidingExpiration="true" requireSSL="false" loginUrl="Login.aspx"
        defaultUrl="Default.aspx"/>
    </authentication>
    <authorization>
      <allow users="*"/>
    </authorization>
    <compilation debug="true" defaultLanguage="C#" targetFramework="4.6">
      <assemblies>
        <!--<clear />-->
        <!--When add tag <clear /> was happen "Could not load type Global"-->
        <add assembly="System.Web, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A"/>
        <add assembly="System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089"/>
